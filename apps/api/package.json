{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:local": "cross-env USE_LOCAL_DB=true dotenv -e ../../.env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "dev": "npm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:migrate:reset": "dotenv -e ../../.env -- prisma migrate reset --force",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio",
    "prisma:seed": "dotenv -e ../../.env -- prisma db seed",
    "update-admin-password": "dotenv -e ../../.env -- npx ts-node -r tsconfig-paths/register scripts/update-admin-password.ts",
    "prisma:seed:local": "dotenv -e ../../.env.seed.local -- prisma db seed",
    "prisma:migrate:local": "dotenv -e ../../.env.seed.local -- prisma migrate deploy",
    "prisma:migrate:status": "dotenv -e ../../.env -- prisma migrate status",
    "set-admins": "ts-node scripts/set-admins.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:serial": "jest --config ./test/jest-e2e.json --runInBand",
    "db:purge": "cd ../.. && node scripts/purgar-db-test.js",
    "rotate-dian-key": "ts-node scripts/rotate-dian-cert-key.ts",
    "verify:tenant-isolation": "dotenv -e ../../.env -- ts-node scripts/verify-tenant-isolation.ts",
    "seed-tenant-sales": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register scripts/seed-tenant-sales.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.0.0",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^5.22.0",
    "@types/archiver": "^7.0.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/node-forge": "^1.3.14",
    "@types/nodemailer": "^7.0.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.5",
    "@types/qrcode": "^1.5.6",
    "@types/swagger-ui-express": "^4.1.6",
    "archiver": "^7.0.1",
    "argon2": "^0.44.0",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "ioredis": "^5.9.2",
    "ml-kmeans": "^7.0.0",
    "node-forge": "^1.3.3",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stripe": "^17.4.0",
    "swagger-ui-express": "^5.0.1",
    "xml-crypto": "^6.1.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^5.20.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
